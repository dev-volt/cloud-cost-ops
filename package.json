    "eslint-config-custom": "workspace:*",
    "prettier": "^2.5.1",
    "turbo": "latest"
  },
  "packageManager": "pnpm@8.6.10",
  "name": "cloud-cost-ops",
  "dependencies": {
    "dotenv-cli": "^7.3.0"
  }
}